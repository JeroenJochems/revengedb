  "keywords": ["laravel", "migration", "generator", "migrations", "artisan", "models", "reverse engineer", "from existent db"],
  "authors": [
    {
      "name": "Diego Armando Avelar",
      "email": "davelar@webeleven.com.br"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "way/generators": "2.*"
  },
  "require-dev": {
    "xethron/migrations-generator": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "Daavelar\\RevengeDb": "src/"
    }
  },
  "minimum-stability": "stable"
}

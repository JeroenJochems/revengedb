{
  "name": "daavelar/revenge-db",
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "4.2.*",
    "way/generators": "2.*"
  },
  "autoload": {
    "psr-0": {
      "Daavelar\\RevengeDb": "src/"
    }
  },
  "authors": [
    {
      "name": "Diego Armando Avelar",
      "email": "davelar@webeleven.com.br"
    }
  ],
  "keywords": [
    "laravel",
    "migration",
    "generator",
    "migrations",
    "artisan",
    "models",
    "reverse engineer",
    "from existent db"
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "phpspec/phpspec": "2.0.*",
    "codeception/codeception": "~2.0",
    "illuminate/console": "~4.2"
  },
  "description": "RevengeDb provides artisan commands to make a reverse engineer of the database, generating automatically the migrations, seeder and models"
}
